  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "vue$": "vue/dist/vue",
      "@components(.*)$": "<rootDir>/app/javascript/components/$1",
      "@locales(.*)$": "<rootDir>/app/javascript/locales/$1",
      "@models(.*)$": "<rootDir>/app/javascript/models/$1",
      "@policies(.*)$": "<rootDir>/app/javascript/policies/$1",
      "@utils(.*)$": "<rootDir>/app/javascript/utils/$1"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "scripts": {
    "test": "TZ=UTC jest"
