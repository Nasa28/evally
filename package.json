{
  "name": "evally",
  "private": true,
  "dependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@rails/webpacker": "^4.0.7",
    "axios": "^0.18.1",
    "chart.js": "^2.9.3",
    "fibers": "^4.0.2",
    "moment": "^2.23.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.3",
    "vue-chartjs": "^3.5.0",
    "vue-clipboard2": "^0.2.1",
    "vue-i18n": "^8.7.0",
    "vue-loader": "^15.8.3",
    "vue-moment": "^4.0.0",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.11",
    "vue-turbolinks": "^2.0.3",
    "vuedraggable": "^2.17.0",
    "vuetify": "^2.2.11",
    "vuetify-loader": "^1.4.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge",
    "cypress": "^4.0.1",
    "webpack-dev-server": "^3.7.2",
    "@vue/test-utils": "^1.0.0-beta.13",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.0",
    "jest": "^21.2.1",
    "jest-transform-stub": "2.0.0",
    "vue-jest": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "@vue(.*)$": "<rootDir>/node_modules/vue-jest",
      "@components(.*)$": "<rootDir>/app/javascript/components/$1",
      "@locales(.*)$": "<rootDir>/app/javascript/locales/$1",
      "@models(.*)$": "<rootDir>/app/javascript/models/$1"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "scripts": {
    "test": "jest"
  }
}
